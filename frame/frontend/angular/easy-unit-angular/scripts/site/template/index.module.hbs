import { NgModule } from '@angular/core';
import { moduleList } from './module';
import { RouterModule } from '@angular/router';
import { {{name}}IndexComponent } from './index.component';
import { TranslateModule } from '@ngx-translate/core';
{{#each components}}
import { {{this.component}} } from './{{this.filename}}';
{{/each}}

@NgModule({
  declarations: [
    {{#each components}}
    {{this.component}},
    {{/each}}
  ],
  imports: [
    TranslateModule,
    ...moduleList,
    RouterModule.forChild([
      { path: '{{path}}', component: {{name}}IndexComponent }
    ])
  ],
  providers: [],
})
export class {{name}}IndexModule { }
